    <plugin name="cordova-android-movetasktoback" spec="^0.1.3" />
    <plugin name="cordova-plugin-appavailability" spec="^0.4.2" />
    <plugin name="WebIntent" spec="https://github.com/Initsogar/cordova-webintent.git" />
    <plugin name="cordova-plugin-inappbrowser" spec="http://scmadmin:scmadmin@172.16.34.221:8080/scm/git/inappbrowser-plugin" />
    <plugin name="ru.softlab.mobile.plugins.ios.properties.list" spec="http://scmadmin:scmadmin@172.16.34.221:8080/scm/git/ios-properties-list" />
