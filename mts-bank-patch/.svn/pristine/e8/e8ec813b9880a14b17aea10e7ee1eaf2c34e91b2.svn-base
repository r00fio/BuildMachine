<div class="list-block favorit-list-box" style="margin: 0;" ng-if="payment.favorite && payment.fromTemplate">
    <ul>
        <li class="item-content">
            <div class="item-media">
                <rs-operation-logo operation="payment.favorite"></rs-operation-logo>
            </div>
            <div class="item-inner">
                <div class="table-fixed-width100pc">
                    <div class="table-row">
                        <div class="table-cell text-15px" ng-bind="payment.favorite.name"></div>
                    </div>
                    <div ng-if="payment.favorite.desc" class="table-row">
                        <span class="table-cell text-12px text-gray" ng-bind="payment.favorite.desc"></span>
                    </div>
                </div>
            </div>
        </li>
    </ul>
</div>