}

.owl-text-theme {
    position: absolute !important;
    width: 200% !important;
    left: -50%;
    top: 16%;
}

.owl-text-theme .owl-controls {
    display: none !important;
}

.owl-text-theme .gallery-text-item {
    width: 100%;
