/*Границы в 1px для retina дисплеев*/
@media (min--moz-device-pixel-ratio: 1.5), (-webkit-min-device-pixel-ratio: 1.5), (min-device-pixel-ratio: 1.5), (min-resolution: 144dpi), (min-resolution: 1.5dppx) {
    .favorit-scroll {
        border: none;
    }
    .favorit-scroll:before {
        content: '';
        position: absolute;
        width: 100%;
        background-color: #CCD5D9;
        height: 1px;
        left: 0;
        top: 0;
        -webkit-transform: scaleY(0.5);
        transform: scaleY(0.5);
    }
    .favorit-scroll:after {
        content: '';
        position: absolute;
        width: 100%;
        background-color: #C7D4D9;
        height: 1px;
        left: 0;
        bottom: 0;
        -webkit-transform: scaleY(0.5);
        transform: scaleY(0.5);
    }
    .payment-flex-container {
        border: none;
    }
    .payment-flex-container:before {
        content: '';
        position: absolute;
        width: 100%;
        background-color: #C7D4D9;
        height: 1px;
        left: 0;
        top: 0;
        -webkit-transform: scaleY(0.5);
        transform: scaleY(0.5);
    }
    .payment-flex-item {
        border: none;
    }
    .payment-flex-item:before {
        content: '';
        position: absolute;
        height: 100%;
        width: 1px;
        right: -0.5px;
        top: 0;
        background-color: #C7D4C9;
        -webkit-transform: scaleX(0.5);
        transform: scaleX(0.5);
    }
    .payment-flex-item:after {
        content: '';
        position: absolute;
        width: 100%;
        height: 1px;
        bottom: 0;
        left: 0;
        background-color: #C7D4C9;
        -webkit-transform: scaleY(0.5);
        transform: scaleY(0.5);
    }
}

