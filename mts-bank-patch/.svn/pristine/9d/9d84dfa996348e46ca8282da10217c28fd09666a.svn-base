body {
    font-family: "Helvetica Neue", "Helvetica Neue Light", "HelveticaNeue-Light", Helvetica, Arial, "Lucida Grande", sans-serif;
    color: #202426;
}

ion-content { background-color: #f0f4f5; }

/* Топбар + кнопки BEGIN */
.bar-header {
    background: #24A7B3 none;
}

.bar-header .title {
    text-transform: uppercase;
    font-size: 14px;
    font-family: "Helvetica Neue Medium", "HelveticaNeue-Medium", Helvetica, Arial, "Lucida Grande", sans-serif;
    font-weight: normal;
    left: 0 !important;
    right: 0 !important;;
}

.bar-header .button,
.bar-header .button:active {
    width: 31px;
    background-position: center !important;
}

.bar-header .button-icon.button-icon,
.bar-header .button-icon.button-icon:active {
    background-size: 17px 17px;
}
/* Левая кнопка в топбаре */
.bar-header .button.button-icon:first-child {
    margin-left: 8px;
}
.bar-header .button.icon-back {
    margin-left: 6px;
}

.bar-header .button.icon-back,
.bar-header .button.icon-back:active {
    background-size: 10px 16px;
}
.bar-header .button.icon-route,
.bar-header .button.icon-route:active {
    background-size: 13px;
}
.bar-header .button.icon-header-arrow {
    background-size: 10px;
    -webkit-transition: -webkit-transform 300ms ease;
    transition: transform 300ms ease;
}
.bar-header .button.icon-header-arrow-up {
    -webkit-transform: rotateZ(180deg);
    transform: rotateZ(180deg);
}
.bar-header .button.icon-menu-more {
    margin-right: 3px;
}

.bar-header .button-icon.button-icon-tuning,
.bar-header .button-icon.button-icon-tuning:active {
    background-size: 13px 13px;
}

/* Правая кнопка в топбаре */
.bar-header .title + .button:last-child {
    right: 13px;
}
.bar-header .title + .button.icon-route:last-child {
    right: 10px;
}

.bar-header button+.title {
    margin: 0 45px;
}

/* Топбар + кнопки END */

.white-spinner .spinner-ios {
    stroke: white;
}

[currency="RUB"] .currency {
    font-family: "CirceRegular", "Helvetica Neue", Helvetica, Arial, "Lucida Grande", sans-serif;
}

[currency="RUB"] .currency-light .currency,
.mts-category-budget-subheader [currency="RUB"] .currency {
    font-family: "CirceLight", "Helvetica Neue Light", "HelveticaNeue-Light", Helvetica, Arial, "Lucida Grande", sans-serif;
}

/* --------------- colors ----------------- */
.text-seagreen { color: #24A7B3; }
.text-gray { color: #A1AEB3; }

/* --------------- bg colors ----------------- */
.bg-seagreen { background-color: #24A7B3; }

.overflow-faded {
    position: relative;
    overflow: hidden;
    white-space: nowrap;
}
.overflow-faded::after {
    content: '';
    position: absolute;
    right: 0;
    top: 0;
    width: 40px;
    height: 100%;
    background: -webkit-linear-gradient(left, rgba(255, 255, 255, 0.2), rgb(255, 255, 255));
    background: linear-gradient(to right, rgba(255, 255, 255, 0.2), rgb(255, 255, 255));
}
.activated .overflow-faded::after {
    background: -webkit-linear-gradient(left, rgba(215, 222, 224, 0.2), rgb(215, 222, 224));
    background: linear-gradient(to right, rgba(215, 222, 224, 0.2), rgb(215, 222, 224));
}

/* Framework7 checkboxes color */
label.label-checkbox input[type=checkbox]:checked+.item-media i.icon-form-checkbox,
label.label-checkbox input[type=radio]:checked+.item-media i.icon-form-checkbox {
    background-color: #24A7B3;
}

.bar-header .button.button-icon.icon-small {
    background-position: center 55%;
}

/* --------- rs-header popup menu styles START --------- */
.rs-popup-menu-header {
    position: absolute;
    left: 0;
    width: 100%;
    -webkit-transform: translateY(-100%);
    transform: translateY(-100%);
    -webkit-transition: -webkit-transform 300ms ease;
    transition: transform 300ms ease;
    z-index: 3;
}
.rs-atm-filter .item-content,
.rs-popup-menu .item-content {
    font-size: 15px;
}
.rs-popup-menu-header.rs-popup-menu-visible {
    -webkit-transform: translateY(0);
    transform: translateY(0);
}
.rs-popup-menu-header .list-block { margin: 0; }
.rs-popup-menu-header .list-block ul { background-color: rgba(255,255,255,0.93); }
.rs-popup-menu-header .list-block .item-inner .rs-header-menu-item-text {
    width: 100%;
    text-align: center;
}
/* --------- rs-header popup menu styles END   --------- */

.popup-container .popup {
    border-radius: 6px;
    background-color: #F9F9F9;
}

.popup-container .popup .popup-head { border-bottom: none; }

.popup-container .popup .popup-body {
    padding: 5px 15px 15px 15px;
}

.popup-container .popup .popup-body input {
    border: 1px solid #667980;
    border-radius: 0;
    text-indent: 15px;
}

.popup-container .popup .popup-body input::-webkit-input-placeholder {
    color: #667980;
    text-indent: 15px;
}

.popup-container.alert .popup {
    width: 270px;
    color: #000000;
    background-color: #FFFFFF;
}

.popup-container.alert .popup .popup-body {
    white-space: pre-wrap;
    font-size: 14px;
    text-align: center;
}

.popup-container.alert .popup-title {
    font-size: 16px;
    font-weight: bold;
}

.popup-container .popup .popup-buttons {
    min-height: 45px;
    font-size: 16px;
    padding: 0;
}

.popup-container .popup .popup-buttons .button {
    border-radius: 0;
    border: none;
    border-top: 1px solid lightgray;
    margin: 0;
}
.popup-container .popup .popup-buttons .button:not(:last-child) {
    border-right: 1px solid lightgray;
}

.popup-container .popup .popup-buttons .button.activated {
    background-color: rgba(0,122,255,.15);
}

.popup-container .popup .popup-buttons .button:hover {
    color: #007aff;
}

.rs-panel-header-title {
