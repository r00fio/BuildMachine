/* Ввод короткого кода */
.icon-back-short-code {
    position: absolute;
    top: 41px;
    width: 10px;
    height: 17px;
    left: 20px;
    background-size: contain;
}
.icon-back-short-code:after {
    content: '';
    display: block;
    position: absolute;
    left: -17px;
    right: -17px;
    top: -13.5px;
    bottom: -13.5px;
}
.short-code-block {
    position: absolute;
    bottom: 0;
    height: auto;
    width: 100%;
    text-align: center;
}
.enter-short-code {
    color: #FFFFFF;
    font-size: 13px;
    width: 100%;
    text-align: center;
    font-family: "Helvetica Neue Light", HelveticaNeue-Light;
}
.cancel-short-code {
    color: #FFFFFF;
    font-size: 14px;
    width: auto;
    text-align: center;
    margin-top: 20px;
    margin-bottom: 28px;
    font-family: "Helvetica Neue Light", HelveticaNeue-Light;
    position: relative;
    display: inline-block;
}
.cancel-short-code:after {
    content: '';
    display: block;
    position: absolute;
    left: -12px;
    right: -12px;
    top: -12px;
    bottom: -12px;
}
.short-code-indicator {
    width: 10px;
    height: 10px;
    border-radius: 100%;
    background-color: white;
    opacity: 0.3;
    margin: 0;
    margin-left: 10px;
    margin-right: 10px;
    margin-top: 20px;
    margin-bottom: 16px;
}
