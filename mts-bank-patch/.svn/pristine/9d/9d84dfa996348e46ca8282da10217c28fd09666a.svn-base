    margin: 0 auto;
    background: url('../img/ios/btn_hide.svg') no-repeat center center;
    background-size: 60%;
}

.not-expandable{
    font-family: "Helvetica Neue";
    color: #667980 !important;
    -webkit-justify-content: center !important;
}

.not-expandable .expandable-icon{
    display: none;
}

.product-slide-box {
    height: 210px;
    min-width: 250px;
    padding-top: 20px;
}

.product-slide-box i {
    color: white;
    font-size: 7px;
}

.mts-product-carousel .swiper-pagination-bullet {
    width: 6px;
    height: 6px;
    margin: 0 2px;
}
.mts-product-carousel .swiper-pagination-bullet.swiper-pagination-bullet-active { background: white; }

.product-block {
    position: relative;
    border-radius: 7px;
    padding: 5px 10px;
    width: 245px;
    height: 150px;
    margin: auto;
    background: #52C2CC no-repeat;
    background-size: cover;
}

/* Фикс для iOS < 8 */
.ios-7 .product-block {
    background-size: 100% auto;
}

.rs-product-carousel-container {
    width: calc(100% + 500px);
    margin-left: -250px;
}

.rs-product-slide-container {
    padding: 5px 0;
}

.rs-product-slide-left-link {
    position: absolute;
    width: 9px;
    height: 30px;
    left: 0;
    top: 50%;
    margin-top: -15px;
    background: url('../img/ios/connect_ico.svg') no-repeat -9px center;
}

.rs-product-slide-right-link {
    position: absolute;
    width: 9px;
    height: 30px;
    right: 0;
    top: 50%;
    margin-top: -15px;
    background: url('../img/ios/connect_ico.svg') no-repeat right -9px center;
}

.product-info {
    background-color: #24A7B3;
    /*height: inherit;*/
}
.product-info:before, .home-header:before {
    content: '';
    display: block;
    position: absolute;
    width: 100%;
    height: 100vh;
    left: 0;
    top: -100vh;
    background-color: inherit;
}

/* Кнопка "Информация" на странице продукта */
.btn-product-info {
    width: 50%;
    height: 65px;
    border: 1px solid #52C2CC;
    border-left: none;
    color: white;
    text-align: center;
    box-sizing: border-box;
    display: table-cell;
    vertical-align: middle;
}

.btn-product-info.activated {
    background-color: #52C2CC;
}

/* Кнопка "Услуги и сервисы" на странице продукта */
.btn-product-services {
    width: 50%;
    height: 65px;
    border: 1px solid #52C2CC;
    border-left: none;
    border-right: none;
    color: white;
    text-align: center;
    box-sizing: border-box;
    display: table-cell;
    vertical-align: middle;
}

.btn-product-services.activated {
    background-color: #52C2CC;
}

/* Длинное название продукта "обрубается" многоточием */
.product-name {
    display: inline-block;
    height: 100%;
    width: 100%;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
}

/* Продукт - Элемент списка START */

.product-name-list {
    max-width: calc(100vw - 85px);
    position: relative;
    overflow: hidden;
    white-space: nowrap;
    font-size: 15px;
}

.fade {
    position: absolute;
    top: 0;
    width: 40px;
    height: 100%;
    background: -webkit-linear-gradient(left, rgba(255, 2255, 255, 0.2), rgb(255, 255, 255));
    background: linear-gradient(to right, rgba(255, 2255, 255, 0.2), rgb(255, 255, 255));
}

.icon-status-product {
    width: 15px;
    height: 15px;
    position: relative;
    background-position: center center;
    background-size: 10px 10px;
}

.right-item-product {
    position: absolute;
    right: 0;
    height: 100%;
}

.card-type-image-product {
    max-height: 15px;
    max-width: 40px;
}

.loan-rest-product {
    color: #A1AEB3;
    font-size: 12px;
    letter-spacing: 0.3px;
    margin-left: 15px;
}

/* Продукт - Элемент списка END */

/* Размер иконки типа карты Visa or MasterCard*/
.product-card-type-label {
    width: 40px;
    height: 25px;
    float: right;
}

.product-card-type-image {
    max-height: 25px;
    background-color: #FFF !important;
    padding: 4px;
    border-radius: 2px;
}

/* ячейка таблицы параметров продукта */
.product-info-cell {
    height: 45px;
    border-bottom: 1px solid #52C2CC;
    padding-left: 15px;
    padding-right: 15px;
}

.product-info-row.activated .product-info-cell {
    background-color: #52C2CC;
}

/* левый лэйбл название параметра */
.product-param-name {
    color: #125359;
}

/* правый лэйбл значение параметра */
.product-param-value {
    font-size: 16px;
    color: white;
    white-space: nowrap;
}

.light-tabs {
    display: -webkit-box;
    display: -webkit-flex;
    display: -ms-flexbox;
    display: flex;

    -webkit-box-pack: center;
    -webkit-justify-content: center;
    -ms-flex-pack: center;
    justify-content: center;

    padding: 7px;
    white-space: nowrap;
}
.light-tabs.dark-tab-theme { background-color: #00ACB5; }

.light-tabs .light-tab-title {
    -webkit-box-flex: 1; /* OLD - iOS 6-, Safari 3.1-6 */
    -moz-box-flex: 1; /* OLD - Firefox 19- */
    -webkit-flex: 1; /* Chrome */
    -ms-flex: 1; /* IE 10 */
    flex: 1;

    display: -webkit-box;
    display: -webkit-flex;
    display: -ms-flexbox;
    display: flex;

    -webkit-box-pack: center;
    -webkit-justify-content: center;
    -ms-flex-pack: center;
    justify-content: center;

    -webkit-box-align: center;
    -webkit-align-items: center;
    -ms-flex-align: center;
    align-items: center;

    color: #667980;
    border-left: 1px solid #CCD5D9;
    border-top: 1px solid #CCD5D9;
    border-bottom: 1px solid #CCD5D9;
    text-align: center;
    padding: 7px;
    font-size: 13px;
}
.light-tabs.dark-tab-theme .light-tab-title{
    color: rgba(255,255,255,0.5);
    border-left: 1px solid rgba(255,255,255,0.5);
    border-top: 1px solid rgba(255,255,255,0.5);
    border-bottom: 1px solid rgba(255,255,255,0.5);
}

.light-tabs .light-tab-title:first-child {
    border-top-left-radius: 5px;
    border-bottom-left-radius: 5px;
}

.light-tabs .light-tab-title:last-child {
    border-top-right-radius: 5px;
    border-bottom-right-radius: 5px;
    border-right: 1px solid #CCD5D9;
}
.light-tabs.dark-tab-theme .light-tab-title:last-child{
    border-right: 1px solid rgba(255,255,255,0.5);
}

.light-tabs .light-tab-title.active {
    background-color: #CCD5D9;
}
.light-tabs.dark-tab-theme .light-tab-title.active {
    color: white;
    background-color: rgba(255,255,255,0.5);
    border: 1px solid transparent;
}


.btn-show-extract {
    border: none;
}

.btn-show-extract.activated {
    background-color: #D7DEE0;
}

/* кнопка "Пополнить" */
.btn-refill {
    position: relative;

    display: -webkit-box;
    display: -webkit-flex;
    display: -ms-flexbox;
    display: flex;

    -webkit-box-pack: center;
    -webkit-justify-content: center;
    -ms-flex-pack: center;
    justify-content: center;

    -webkit-box-align: center;
    -webkit-align-items: center;
    -ms-flex-align: center;
    align-items: center;

    height: 64px;
    color: white;
    background-color: #52C2CC;
}
.btn-refill.plain-button-style { background-color: #CCD5D9; }
.btn-refill.colored-button-style { background-color: #24A7B3; }
.btn-refill button {
    position: relative;
    width: 100%;
    height: 100%;
    line-height: 64px;
    border: none;
    color: white;
    background-color: transparent;
}
.btn-refill.plain-button-style button { color: #66777F; }
.btn-refill button.activated { background-color: rgba(255,255,255,0.15); }

.operations {
    width: 100%;
    height: 100%;
    background: #F5F5F5;
}

.operations-title {
    width: 100%;
    height: 8vh;
    line-height: 8vh;
    color: #A1AEB3;
    padding-left: 15px;
